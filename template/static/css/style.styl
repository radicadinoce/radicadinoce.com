@import('nib')

global-reset()

html,
body
  width 100%
  height 100%
  font-family monospace
  
body
  position relative
  
.pics
  position absolute
  top 50%
  left 50%
  transform translate(-50%, -50%)
  text-align center
  img
    display inline-block
    width 60px
    height auto
    max-width 29%
    margin 0 2%